#include<stdio.h>
#include<stdlib.h>
int main()
{
int a[10][10],b[10][10],c[10][10],r1,c1,r2,c2,i,j,k;
printf("enter a matrix rows and columns");
scanf("%d%d",&r2,&c2);
if(r1!=0)
{
printf("matrix multiplication is not possible");
exit(1);
}
printf("enter a matrix values");
for(i=0;i<r1;i++)
{
for(j=0;j<c1;j++)
{
scanf("%d",&a[i][j]);
}
}
printf("enter b matrix values");
for(i=0;i<r2;i++)
{
for(j=0;j<c2;J++")
{
scanf("%d",&b[i][j]);
}
}
for(i=0;i<r1;i++)
{
for(j=0;j<c1;j++)
{
c[i][i]=0;
for(k=0;k<r2;k++)
{
c[i][j]=c[i][j]+a[i][k]*b[k][j];
}
}
}
printf("result matrix \n");
for(i=0;i<r;i++)
{
for(j=0;j<r;j++)
{
printf("%d",c[i][j]);
}
printf("\n");
}
return 0;
}
